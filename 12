A = ["Emin", "Emir", "İclal", "Bekir", "Cem", "Arda", "Kaan", "Metin", "Can", "Ramiz"]
B = [56, 78, 95, 23, 95, 87, 61, 77, 45, 33]

maxGrade = max(B)
minGrade = min(B)

print("En Yüksek Notu Alan Kişi/Kişiler:")
for index, grade in enumerate(B):
    if grade == maxGrade:
        print(A[index])

print("\nEn Düşük Notu Alan Kişi/Kişiler:")
for index, grade in enumerate(B):
    if grade == minGrade:
        print(A[index])
